{
  "name": "iobroker.airquality",
  "version": "0.1.4",
  "description": "Fetch data from German UBA",
  "author": {
    "name": "raschy",
    "email": "raschy@gmx.de"
  },
  "homepage": "https://github.com/raschy/ioBroker.airquality",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "Airquality",
    "UBA",
    "particles",
    "NOx",
    "CO2",
    "O3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/raschy/ioBroker.airquality.git"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.2.3"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.2",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@iobroker/adapter-dev": "^1.3.0",
    "@iobroker/eslint-config": "^1.0.0",
    "@iobroker/testing": "^5.0.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/chai": "^4.3.19",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.8.6",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "mocha": "^10.7.3",
    "proxyquire": "^2.1.3",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).{json,json5}",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "build-adapter ts",
    "watch": "build-adapter ts --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint_": "eslint --ext .ts src/",
    "lint": "eslint -c eslint.config.mjs .",
    "checker": "node skript.mjs",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/raschy/ioBroker.airquality/issues"
  },
  "readmeFilename": "README.md"
}
